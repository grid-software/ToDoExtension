{
  "manifest_version": 3,
  "name": "To-Do Browserextension",
  "version": "0.0.0.1",
  
  "action": {
      "default_popup": "index.html",
      "default_title": "To-Do"
  },
  "description": "Introducing a cutting-edge browser extension designed for seamless integration with your Microsoft To-Do account",
  
  "permissions": [
      "identity",
      "activeTab",
      "webNavigation",
      "tabs"
      

  ],
  "icons": {
  "128": "icons1282.png"
    }
  ,

  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}